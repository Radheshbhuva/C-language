#include<stdio.h>
#include<conio.h>
#define pf printf
#define sf scanf
#define cl clrscr
main()
{
	int num1, num2, num3, min;
	clrscr();
	//taking the input from users
	pf("Enter the First num value of num1 : ");
	sf("%d", &num1);
	pf("Enter the Second num value of num2 : ");
	sf("%d", &num2);
	pf("Enter the third num value of num3 : ");
	sf("%d", &num3);

	//Now using nested if-else for finding minimum number
	pf("\n\n");

	if(num1 < num2){
	  if (num1 < num3){
		min = num1;
	  } else {
		min = num3;
	  }
}else {
	if (num2 < num3){
	      min = num2;
	} else {
	      min = num3;
	}
}

	pf("The minimum value is : %d\n", min);
	getch();
}

