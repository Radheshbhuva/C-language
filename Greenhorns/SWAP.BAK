#include <stdio.h>
#include <conio.h>
#define pf printf
#define sf scanf
main()
{
	int a, b;
	clrscr();

	//Take the value of 'a' and 'b'from User.

	pf("Put the Value of a : ");
	sf("%d" ,a);

	pf("Put the Value of b : ");
	sf("%d" ,b);

	//Steps of Swapping Two Variable.
	pf("Before swapping : a = %d, b = %d\n", a, b);

	a = a^b;
	b = a^b;
	a = a^b;

	pf("After Swapping : a = %d, b = %d\n", a, b);
	getch();
}




